{
  "kind": "collectionType",
  "collectionName": "leads",
  "info": {
    "singularName": "lead",
    "pluralName": "leads",
    "displayName": "Leads"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "phone": {
      "type": "string",
      "minLength": 9,
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "leads_status": {
      "type": "enumeration",
      "enum": [
        "new",
        "contacted",
        "qualified",
        "lost"
      ]
    },
    "client": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::client.client",
      "mappedBy": "lead"
    }
  }
}
